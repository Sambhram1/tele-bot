{
  "name": "telegram-ai-image-editor",
  "version": "1.0.0",
  "description": "A Telegram bot for AI-powered image editing with background removal, upscaling, and basic edits",
  "main": "index.js",
  "scripts": {
    "start": "node index.js",
    "dev": "nodemon index.js",
    "setup": "node setup.js",
    "test": "node test.js",
    "check": "node test.js",
    "deploy": "wrangler deploy",
    "deploy:staging": "wrangler deploy --env staging",
    "deploy:production": "wrangler deploy --env production",
    "deploy:cloudflare": "node deploy-cloudflare.js",
    "cf:dev": "wrangler dev",
    "cf:login": "wrangler login",
    "cf:setup": "npm run cf:login && npm run cf:secret",
    "cf:secret": "wrangler secret put BOT_TOKEN",
    "cf:webhook": "node scripts/setup-webhook.js"
  },
  "keywords": [
    "telegram",
    "bot",
    "image-editing",
    "ai",
    "rembg",
    "sharp"
  ],
  "author": "Your Name",
  "license": "MIT",
  "dependencies": {
    "axios": "^1.6.0",
    "dotenv": "^16.3.1",
    "fs-extra": "^11.1.1",
    "sharp": "^0.34.3",
    "telegraf": "^4.15.6",
    "uuid": "^9.0.1"
  },
  "devDependencies": {
    "nodemon": "^3.0.1",
    "wrangler": "^3.114.14"
  },
  "engines": {
    "node": ">=16.0.0"
  }
}
